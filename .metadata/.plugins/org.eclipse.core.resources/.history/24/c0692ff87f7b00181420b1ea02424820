package idv.camille.assignment;

public class MyApplication {
	public static void main(String[] args) {
		// #1 Runnable
		
		//#2 Thread
		MyThread mt = new MyThread(); /* main thread created the child thread*/
		mt.start();
		for(int i=0; i<10; i++){
		System.out.println("Main Thread");
		}
		
	}
}
